function deleteSSOModifyPwdUid(){
    //同步调用
    var _r = $ajax("ajax/AjaxDeleteSSOModifyPwdUser.aspx?t="+hidT.value+"&tmp="+Math.floor(Math.random()*1000),null);
    if(_r=="T"){
        resetWin();
        ssoRedirect();
    }else{
        ssoRedirect();
    }
}
//去修改
function gotoModifyPwd(){
    var url=hidS.value;
    if(url.length>0) {
        url = url.replace("accounts.", "sinfo.") + "/myinfo/AccountCenter/AccountSecurity.aspx";
       window.open(url, 'newwindow', 'height=768, width=1024, top=100, left=100, toolbar=yes, menubar=yes, scrollbars=yes, resizable=yes,location=yes, status=yes');
    }
     //ssoRedirect();
}

//找回密码
function gotoFindPwd(){
    var url=hidS.value;
    if(url.length>0){
        url="https://accounts.ctrip.com/member/PassWord/VerifyUserInfo.aspx"+"?uid="+txtUname.value;      
       window.open(url, 'newwindow', 'height=768, width=1024, top=100, left=100, toolbar=yes, menubar=yes, scrollbars=yes, resizable=yes,location=yes, status=yes');
    }
     //ssoRedirect();
}
//设置交易密码
function ModifyTicketPwd(){
    var url=hidS.value;
    
    if(url.length>0){
        url=url.replace("accounts.", "secure.");
        if(url.indexOf("big5")>=0){
            url=url+"/youpiao/TravelTicketBig5/SecuritySetting.aspx";
        }
        else if (url.indexOf("corp")>=0)
        {
            url = url.Replace("corp.", "").Replace("corporatetravel.", "") + "/youpiao/TravelTicket/SecuritySetting.aspx";
        }
        else
        {
            url = url + "/youpiao/TravelTicket/SecuritySetting.aspx";
        }      
       window.open(url, 'newwindow', 'height=768, width=1024, top=100, left=100, toolbar=yes, menubar=yes, scrollbars=yes, resizable=yes,location=yes, status=yes');        
    }
     ssoRedirect();
}

