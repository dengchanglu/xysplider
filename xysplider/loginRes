        var backurl = "http://my.ctrip.com/home/myinfo.aspx?sso_lct=F";
        var isneedmodifypwd = "F";
        function ssoRedirect() {
            if (isneedform == "T" && backurl != undefined && backurl.length > 0) {
                if (responseM == "GET") {
                    if (backurl.indexOf("?") > 0) {
                        window.location.href = backurl;//+ "&token=" + document.formin.token.value;
                    } else {
                        window.location.href = backurl;//+ "?token=" + document.formin.token.value;
                    }
                }
                else {
                    $("formin").action = backurl;
                    $("formin").method = responseM;
                    $("formin").submit();
                }
            }
        }
